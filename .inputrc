$include /etc/inputrc
set bell-style					none
set colored-stats				on
set completion-ignore-case		off
set show-all-if-ambiguous		on
set show-all-if-unmodified		on
set visible-stats				off
set bind-tty-special-chars		on
set mark-symlinked-directories	on

# Use Up/Down for partial history search
"\e[B": history-search-forward
"\e[A": history-search-backward

# Ctrl-Backspace to Undo - not working/needed? Possibly part of wsltty
# "\033[3;5~": "undo"

# M-F11 to null (nested tmux config)
"\033[23;3~": ""
# M-F12 to null (nested tmux config)
"\033[24;3~": ""

# C-PgUp to null
"\033[5;5~": ""
# C-PgDown to null
"\033[6;5~": ""

# M-PgUp to null
"\033[5;3~": ""
# M-PgDown to null
"\033[6;3~": ""

# S-PgUp to null
"\033[5;2~": ""
# S-PgDown to null
"\033[6;2~": ""

# M-Up to null
"\033[1;3A": ""
# M-Down to null
"\033[1;3B": ""
# M-Right to null
"\033[1;3C": ""
# M-Left to null
"\033[1;3D": ""

# S-Up to null
"\033[1;2A": ""
# S-Down to null
"\033[1;2B": ""
# S-Right to null
"\033[1;2C": ""
# S-Left to null
"\033[1;2D": ""

# C-S-Up to null
"\033[1;6A": ""
# C-S-Down to null
"\033[1;6B": ""

# C-M-Up to null
"\033[1;7A": ""
# C-M-Down to null
"\033[1;7B": ""
# C-M-Right to null
"\033[1;7C": ""
# C-M-Left to null
"\033[1;7D": ""

# M-S-Up to null
"\033[1;4A": ""
# M-S-Down to null
"\033[1;4B": ""
# M-S-Right to null
"\033[1;4C": ""
# M-S-Left to null
"\033[1;4D": ""
