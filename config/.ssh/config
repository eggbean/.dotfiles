Host *
	Match localuser u0_a423
		PKCS11Provider /data/data/com.termux/files/usr/lib/libtergent.so
		IdentityFile ~/.ssh/id_android

Host mikrotik
	HostName 192.168.88.1
	IdentityFile ~/.ssh/id_rsa

Host proxima
	HostName 192.168.88.2
	User pi
	IdentityFile ~/.ssh/id_ecdsa
	ForwardAgent yes

Host bastet
	HostName bastet.jinkosystems.co.uk
	User opc
	VerifyHostKeyDNS ask
	ForwardAgent yes

Host osiris
	HostName osiris.jinkosystems.co.uk
	IdentityFile ~/.ssh/id_ecdsa
	VerifyHostKeyDNS ask
	ForwardAgent yes

Host isis
	HostName isis.jinkosystems.co.uk
	IdentityFile ~/.ssh/id_ecdsa
	VerifyHostKeyDNS ask
	ForwardAgent yes

Host amunet
	HostName amunet.jinkosystems.co.uk
	ProxyJump bastet.jinkosystems.co.uk
	IdentityFile ~/.ssh/id_rsa
	VerifyHostKeyDNS ask
	ForwardAgent yes
	RemoteForward /run/user/1000/gnupg/S.gpg-agent ${HOME}/.gnupg/S.gpg-agent.extra

Host horus
	HostName horus.jinkosystems.co.uk
	User opc
	ProxyJump bastet.jinkosystems.co.uk
	VerifyHostKeyDNS ask
	ForwardAgent yes
	RemoteForward /run/user/1000/gnupg/S.gpg-agent ${HOME}/.gnupg/S.gpg-agent.extra

Host *.amazonaws.com
	ProxyJump bastet.jinkosystems.co.uk
	IdentityFile ~/.ssh/id_rsa
	RemoteForward /run/user/1000/gnupg/S.gpg-agent ${HOME}/.gnupg/S.gpg-agent.extra

Host github.com
	HostName github.com
	User git

Host *
	User jason
	IdentityFile ~/.ssh/id_ed25519
	IdentitiesOnly no
	VerifyHostKeyDNS no
	StrictHostKeyChecking accept-new
	ForwardAgent no
	StreamLocalBindUnlink yes
	ControlMaster auto
	ControlPersist 8h
	ControlPath ~/.ssh/ssh_control-%n-%p-%r
	Compression yes
	SendEnv LANG
