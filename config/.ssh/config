Match localuser u0_a423
	PKCS11Provider /data/data/com.termux/files/usr/lib/libtergent.so
	IdentitiesOnly no

Match all
	Include ~/.ssh/*.conf

Match host * exec "arp -a 192.168.88.1 2>/dev/null | grep -i c4-ad-34-bd-72-83"
	ProxyJump none

Match exec "onsubnet 10.30."
	ProxyJump none

Host mikrotik
	HostName 192.168.88.1
	IdentityFile ~/.ssh/id_rsa
	ForwardAgent no

Host bastet
	HostName bastet.jinkosystems.co.uk
	ProxyJump none
	ForwardAgent yes
	VerifyHostKeyDNS ask
	ControlMaster yes
	ForwardX11 yes
	ForwardX11Trusted yes
	RemoteForward /var/run/user/1000/gnupg/S.gpg-agent /var/run/user/1000/gnupg/S.gpg-agent.extra
	SendEnv TMUX_SSH_SPLIT

Host vega pegasus osiris amunet
	HostName %h.jinkosystems.co.uk
	ProxyJump bastet.jinkosystems.co.uk
	ForwardAgent yes
	VerifyHostKeyDNS ask
	ForwardX11 yes
	ForwardX11Trusted yes
	RemoteForward /var/run/user/1000/gnupg/S.gpg-agent /var/run/user/1000/gnupg/S.gpg-agent.extra
	SendEnv TMUX_SSH_SPLIT

Host *
	User jason
	IdentityFile ~/.ssh/id_ed25519
	IdentitiesOnly yes
	AddKeysToAgent yes
	ForwardAgent no
	GSSAPIAuthentication no
	VerifyHostKeyDNS no
	StrictHostKeyChecking accept-new
	HashKnownHosts no
	StreamLocalBindUnlink yes
	ServerAliveInterval 5
	ServerAliveCountMax 1
	ControlMaster auto
	ControlPersist 24h
	ControlPath ~/.ssh/ssh_control-%n-%p-%r
	Compression yes
	ForwardX11 no
	ForwardX11Trusted no
	SendEnv LANG LC_*
